{
    "name": "datinglibre/datinglibre-ccbill-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dating Libre CcBill Bundle",
            "email": "admin@datinglibre.com"
        }
    ],
    "description": "DatingLibre CcBill Client",
    "require": {
        "ext-json": "*",
        "ext-simplexml": "*",
        "datinglibre/ccbill": "0.7.*",
        "symfony/framework-bundle": "5.2.*||5.3.*",
        "symfony/serializer": "5.2.*||5.3.*",
        "symfony/http-client": "5.2.*||5.3.*",
        "symfony/console": "5.2.*||5.3.*",
        "symfony/dotenv": "5.2.*||5.3.*",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DatingLibre\\CcBillBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DatingLibre\\CcBillBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/phpunit-bridge": "^5.2",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
